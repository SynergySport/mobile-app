<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TitleTextStyleBlack">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_title</item>
    </style>

    <style name="NormalTextStyleBlack">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_normal</item>
    </style>

    <style name="NormalTextStyleWhite">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_normal</item>
    </style>

    <style name="SmallTextStyleBlack">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_small</item>
    </style>

    <style name="CustomTextInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:maxWidth">346dp</item>
        <item name="boxStrokeColor">#C8C4C4</item>
        <item name="boxCornerRadiusBottomEnd">4dp</item>
        <item name="hintTextColor">@color/black</item>
        <item name="boxCornerRadiusBottomStart">4dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">#09324B</item>
        <item name="boxCornerRadiusTopEnd">4dp</item>
        <item name="boxCornerRadiusTopStart">4dp</item>
        <item name="colorControlActivated">@color/black</item>
    </style>
</resources>